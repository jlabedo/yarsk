{
  "name": "yarsk",
  "version": "1.0.0",
  "description": "My React starter kit. Webpack, Babel, Sass, React 0.13 and more.",
  "main": "app/index.js",
  "repository": "git@github.com:jlabedo/yarsk.git",
  "scripts": {
    "test": "./node_modules/.bin/karma start conf/karma.conf.js --single-run",
    "test:watch": "./node_modules/.bin/karma start conf/karma.conf.js",
    "test:coverage": "./node_modules/.bin/karma start conf/karma.coverage.conf.js --single-run",
    "test:ci": "./node_modules/.bin/karma start conf/karma.ci.conf.js --single-run",
    "start": "./node_modules/.bin/webpack-dev-server --config conf/webpack.config.js --hot --progress --colors --inline --content-base ./build",
    "build": "./node_modules/.bin/webpack --config conf/webpack.production.js",
    "build:gh": "npm run build && ./node_modules/.bin/gh-pages -d ./dist && rm -r ./dist",
    "clean:dist": "rm -rf dist",
    "clean:tmp": "rm -rf tmp",
    "clean:coverage": "rm -rf coverage",
    "clean": "npm run clean:dist && npm run clean:tmp && npm run clean:coverage"
  },
  "author": "Brad Daily <brad@bradleyboy.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.1.10",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.0.0",
    "chai": "^3.3.0",
    "css-loader": "^0.19.0",
    "cssnext": "^1.8.4",
    "eslint": "^1.6.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.1.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.1",
    "gh-pages": "^0.2.0",
    "html-webpack-plugin": "^1.1.0",
    "isparta": "^3.1.0",
    "isparta-instrumenter-loader": "^0.2.1",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-notify-reporter": "^0.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.5.0",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.3.3",
    "phantomjs": "^1.9.18",
    "postcss-import": "^7.0.0",
    "postcss-loader": "^0.6.0",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.2.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.0",
    "webpack-dev-server": "^1.9.0"
  }
}
